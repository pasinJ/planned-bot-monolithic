{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cy": "NODE_ENV=test vite",
    "test": "DOTENV_CONFIG_PATH=.env/.env.dev jest",
    "test:u": "npm run test -- --selectProjects DOM NODE --testPathPattern",
    "test:d": "npm run test -- --selectProjects DOM --testPathPattern",
    "test:n": "npm run test -- --selectProjects NODE --testPathPattern",
    "test:i": "npm run test -- --selectProjects INTEGRATION --testPathPattern",
    "test:w": "npm run test:u -- --watch",
    "cy:open": "cypress open -b chrome --e2e",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "build:clean": "rimraf dist && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache .",
    "prettier": "prettier . --write --cache --log-level warn"
  },
