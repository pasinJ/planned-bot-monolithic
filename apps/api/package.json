{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=.env/.env.dev tsx --require dotenv/config src/main.ts",
    "dev:w": "DOTENV_CONFIG_PATH=.env/.env.dev tsx watch --require dotenv/config src/main.ts",
    "test": "DOTENV_CONFIG_PATH=.env/.env.dev tsx --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:u": "npm run test -- --selectProjects UNIT --testPathPattern",
    "test:i": "npm run test -- --selectProjects INTEGRATION --testPathPattern",
    "test:e": "npm run test -- --selectProjects E2E --testPathPattern",
    "test:w": "npm run test:u -- --watch --testPathPattern",
    "start": "node --experimental-specifier-resolution=node --no-warnings dist/main.js",
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "build:clean": "rimraf dist && npm run build",
    "lint": "eslint . --ext .ts --cache .",
    "prettier": "prettier . --write --cache --log-level warn",
    "prisma:gen": "prisma generate",
    "prisma:push": "prisma db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.2",
    "@faker-js/faker": "^8.0.2",
    "@relmify/jest-fp-ts": "^2.1.1",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.28",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@tsconfig/node-lts": "^18.12.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@types/ramda": "^0.29.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.1",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "fastify": "^4.21.0",
    "fp-ts": "^2.16.1",
    "fp-ts-std": "^0.17.1",
    "nanoid": "^3.3.6",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "ramda": "^0.29.0",
    "ts-custom-error": "^3.3.1",
    "ts-pattern": "^5.0.5",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  }
}
