{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=env/.env.dev tsx --require dotenv/config src/main.ts",
    "dev:w": "DOTENV_CONFIG_PATH=env/.env.dev tsx watch --require dotenv/config src/main.ts",
    "dev:t": "DOTENV_CONFIG_PATH=env/.env.test tsx watch --require dotenv/config src/main.ts",
    "test": "TZ=UTC DOTENV_CONFIG_PATH=env/.env.dev tsx --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:u": "npm run test -- --selectProjects UNIT --testPathPattern",
    "test:uc": "npm run test:u -- --onlyChanged",
    "test:uw": "npm run test:uc -- --watch",
    "test:i": "npm run test -- --selectProjects INTEGRATION --runInBand --testPathPattern",
    "test:ic": "npm run test:i -- --onlyChanged",
    "test:iw": "npm run test:ic -- --watch",
    "test:e": "npm run test -- --selectProjects E2E --runInBand --testPathPattern",
    "test:ec": "npm run test:e -- --onlyChanged",
    "test:w": "concurrently -c 'auto' -P npm:test:uw npm:test:iw",
    "start": "node --experimental-specifier-resolution=node --no-warnings dist/api/src/main.js",
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "build:clean": "rimraf dist && npm run build",
    "lint": "npm run lint:git .",
    "lint:git": "eslint --fix --cache --max-warnings=0",
    "prettier": "npm run prettier:git .",
    "prettier:git": "prettier --write --ignore-unknown --cache --log-level warn",
    "webpack": "webpack --config webpack/webpack.config.cjs",
    "sonar": "node sonarscan.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@relmify/jest-fp-ts": "^2.1.1",
    "@swc/core": "^1.3.92",
    "@swc/jest": "^0.2.28",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@tsconfig/node-lts": "^18.12.3",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.4.8",
    "@types/ramda": "^0.29.3",
    "@types/unzipper": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.1",
    "lint-staged": "^14.0.1",
    "msw": "^1.2.3",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "sonarqube-scanner": "^3.1.0",
    "swc-loader": "^0.2.3",
    "ts-essentials": "^9.4.0",
    "tsc-alias": "^1.8.7",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@ixjb94/indicators": "^1.1.22",
    "agenda": "^5.0.0",
    "axios": "^1.4.0",
    "binance-api-node": "^0.12.6",
    "console-stamp": "^3.1.2",
    "csv-parse": "^5.5.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "decimal.js": "^10.4.3",
    "fastify": "^4.21.0",
    "fp-ts": "^2.16.1",
    "fp-ts-std": "^0.17.1",
    "isolated-vm": "^4.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.4.3",
    "nanoid": "^3.3.6",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "ramda": "^0.29.0",
    "ts-pattern": "^5.0.5",
    "unzipper": "^0.10.14",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.5.0"
  }
